<section class="breadcrumb-option">
  <div class="container" style="height:2rem;">
    <div class="row">
      <div class="col-lg-12">
        <div class="breadcrumb__text">
                  <h2>collection</h2>
          <div class="breadcrumb__links">
            <a href="/">Home</a>
            <a href="/"><span>Shop</span></a>
            <span>{{categoryName}}</span>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>


<section class="shop spad">
  <div class="container">
    <div class="row">
      <div class="col-lg-3">
        <div class="shop__sidebar">
          <div class="shop__sidebar__search">
<form action="/user/search" method="get">
                <input
                class="search_input"
                type="search"
                name="search"
                placeholder="Search..."
              />
              <button type="submit" class="search_icon"><i class="fas fa-search"></i></button>
              </form>
          </div>
          <div class="shop__sidebar__accordion">
            <div class="accordion" id="accordionExample">
              <div class="card">

              </div>
              <div class="card">

                <div id="collapseTwo" class="collapse show" data-parent="#accordionExample">
                  <div class="card-body">
                    <div class="shop__sidebar__brand">
                      <ul>
                        {{#each brands}}
                        <li><a href="/products?brandId={{this._id}}">{{this.brand}}</a></li>
                        {{/each}}
                      </ul>
                    </div>
                  </div>
                </div>
              </div>

            </div>
          </div>
        </div>
      </div>
      <div class="col-lg-9">
        <div class="shop__product__option">
          <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6">
              <div class="shop__product__option__left">
              </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6">
              <div class="shop__product__option__right">
                <div class="dropdown">
                  <button class=" dropdown-toggle border-0 opacity-25" type="button" id="dropdownMenuButton1"
                    data-bs-toggle="dropdown" aria-expanded="false">
                  </button>

                </div>
              </div>
            </div>
          </div>
        </div>
        
        <div class="row">
          {{#if searchProduct}}
          {{#each searchProduct}}
          <div class="col-lg-4 col-md-6 col-sm-6">
            <div class="product__item">
            <a href="/user/productDetails/{{this._id}}">
            <div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
            <span class="label"><s class="text-danger" style="font-size:1rem;">₹{{this.actualPrice}}</s></span>
            <ul class="product__hover">

            <li><a href="/user/productDetails/{{this._id}}"><img src="data:image/png;base64," alt="" data-pagespeed-url-hash="3756334862" onload="pagespeed.CriticalImages.checkImageForCriticality(this);"></a><a href="/user/productDetails/{{this._id}}"><span>View Product</span></a></li>
            </ul>
            </div>
            </a>
            <div class="product__item__text">
            <a href="/user/productDetails/{{this._id}}"></a>
            <h6>{{this.name}}</h6>
            <a class="add-cart"><button onclick="addToCart('{{this._id}}')" style="background:none; border:none ;">Add to Bag <i class="fa fa-shopping-cart"></i></button></a>
            <h5>Rs.{{this.offerPrice}}</h5>
            </div>
            </div>
          </div>
          {{/each}}
          {{else}}
          <section>
                        <div class="d-flex justify-content-center vh-100">
                            <div class="text-center">
                                <p class="fs-3"> <span class="text-danger">Opps!</span> No Items found.</p>
                                <p class="lead">
                                    The product you’re looking for doesn’t exist.
                                </p>
                                <a href="/" class="btn btn-dark">Home Page</a>
                            </div>
                        </div>
                    </section>
          {{/if}}

        </div>
        <div class="row">
          <div class="col-lg-12">
            <div class="product__pagination">
              {{#each page}}
              <a class="active" href="/products?&p={{this}}">{{this}}</a>
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>





<div class="search-model">
  <div class="h-100 d-flex align-items-center justify-content-center">
    <div class="search-close-switch">+</div>
    <form class="search-model-form">
      <input type="text" id="search-input" placeholder="Search here.....">
    </form>
  </div>
</div>


<script>
  //sort
  function sort(number) {
    let url = new URL(window.location.href);
    let params = new URLSearchParams(url.search);
    params.set('sort', number);
    let newParams = params.toString();
    location.href = window.location.protocol + "//" + window.location.host + '/products?' + newParams;
  }
</script>


